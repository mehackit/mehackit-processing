void setup() {
    size(500, 500);
}

void draw() {
    background(255);
     
    strokeWeight(50);
    stroke(30, 50);
    noFill();
     
    rect(30 + 80, 30 + 80, 80, 80);
    rect(30 + 40, 30 + 40, 160, 160);
    rect(30 + 40, 30 + 40, 160, 160);
    rect(30 + 80, 30 + 80, 240, 240);
     
    rect(30 + 280, 30 + 280, 40, 40);
    rect(30 + 320, 30 + 320, 80, 80);
}